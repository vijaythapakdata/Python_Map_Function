        "def square(x):\n",
        "  return x*x\n",
        "\n",
        "nums=[1,2,3,4,5,6]\n",
        "\n",
        "nums_squared=map(square,nums) #here square is function and nums is iterable\n",
        "\n",
        "for num in nums_squared:\n",
        "  print(num)\n"
      ],
